<UserControl x:Class="VPet.Plugin.LetsPlayIt.winApp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:pu="https://opensource.panuon.com/wpf-ui"
        xmlns:local="clr-namespace:VPet.Plugin.LetsPlayIt"
        Width="70" Height="70" FontSize="14" Margin="0,0,0,0" VerticalAlignment="Top"
        HorizontalAlignment="Center">
    <Grid Name="Canvas">
        <Image Width="Auto" Height="Auto" HorizontalAlignment="Center" MouseDown="OpenApp" VerticalAlignment="Top" x:Name="AppImage" Margin="0,0,0,0"/>
    </Grid>
</UserControl>